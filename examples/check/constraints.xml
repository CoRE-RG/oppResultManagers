<?xml version="1.0"?>
<constraints xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="constraints.xsd">
	<constraint module="Network.node1" name="rxMessageAge:vector">
		<min>1.5</min>
		<max>1.7</max>
	</constraint>
	<constraint module="(.*)\.node2" moduleIsRegex="true" name="(rx|tx)MessageAge:vector" nameIsRegex="true">
		<avg_min samples="10">1.5</avg_min>
		<avg_max samples="10">1.7</avg_max>
	</constraint>
</constraints>